{
    "builds": [
        {
            "src": "api/**/*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/.*",
            "dest": "/api/$1.py"
        },
        {
            "src": "/",
            "dest": "/terminal.html"
        }
    ]
}